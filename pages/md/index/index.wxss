}
.item3 label{
    position: relative
}
.seemore::after{
	position: absolute;
	top: 50%;
	right: -30rpx;
	width: 10rpx; 
	height: 10rpx;
	content: "";
	margin-top: -5rpx;
	border-bottom: 2rpx solid #6a7fa5;
	border-right: 2rpx solid #6a7fa5;
	-webkit-transform: rotate(45deg);
	-moz-transform: rotate(45deg);
	-o-transform: rotate(45deg);
	transform: rotate(45deg);
}
.fold::after{
	position: absolute;
	top: 50%;
	right: -30rpx;
	width: 10rpx; 
	height: 10rpx;
	content: "";
	margin-top: -5rpx;
	border-top: 2rpx solid #6a7fa5;
	border-left: 2rpx solid #6a7fa5;
	-webkit-transform: rotate(45deg);
	-moz-transform: rotate(45deg);
	-o-transform: rotate(45deg);
	transform: rotate(45deg);
}
.notice {
    position: relative;
    background: #fff;
    font-size: 24rpx;
    line-height: 2;
    color: #666;
    padding: 30rpx;
    text-align: left
}
.group_item {
    position: relative;
