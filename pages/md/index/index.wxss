}
.g_seeDetail text::after{
	position: absolute;
	top: 50%;
	right: 0;
	width: 10rpx; 
	height: 10rpx;
	content: "";
	margin-top: -5rpx;
	border-bottom: 2rpx solid #6a7fa5;
	border-right: 2rpx solid #6a7fa5;
	-webkit-transform: rotate(-45deg);
	-moz-transform: rotate(-45deg);
	-o-transform: rotate(-45deg);
	transform: rotate(-45deg);
}
