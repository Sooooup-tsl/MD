/* 弹框 */
.layer {
    background: rgba(0,0,0,0.7);
    height: 100%;
    width: 100%;
    position: fixed;
    left: 0;
    top: 0;
    z-index: 100
}
.pop_box {
    position: fixed;
    width: calc(100% - 60px);
    top: 100rpx;
    left: 30px;
    margin: auto;
    background: #fff;
    padding: 40rpx 30rpx;
    box-sizing: border-box;
    border-radius: 10rpx;
    z-index: 101;
}
.pop_box .close{
    position:absolute;
    left:50%;
    margin-left: -50rpx;
    bottom:-150rpx;
}
.pop_box .title {
    font-size: 36rpx;
    text-align: center;
}
.pop_content label{
    display: inline-block;
    font-size: 28rpx;
    width: 100rpx;
    height:68rpx;
    line-height: 68rpx;
    text-align: left;
    vertical-align: top
}
.pop_content input {
    display: inline-block;
    width: 462rpx;
    background: #efeff4;
    height: 68rpx;
    font-size: 26rpx;
    padding-left: 30rpx;
    box-sizing: border-box
}
.pop_content .sex_box {
    display:inline-block;
    width:462rpx;
    background:#efeff4;
    height:68rpx;
    font-size:26rpx;
    padding-left:30rpx;
    box-sizing:border-box;
    vertical-align: top

}
.pop_content .send{
    position:absolute;
    top:9rpx;
    right:30rpx;
    color:#fff;
    font-size:24rpx;
    height:50rpx;
    line-height: 50rpx;
    width:140rpx;
    background: #fe3a2e;
    border:0;
    margin:0;
    padding:0;
    z-index:100;


}
.pop_box .submit{
    color: #fff;
    font-size: 30rpx;
    height: 75rpx;
    background: #fe3a2e;
    border: 0;
    margin: 0;
    padding: 0
}
.mb-40 {
    position: relative;
